{
  "name": "all-tickers-bulk",
  "version": "2.0.0",
  "description": "Bulk ticker generation and validation system with SQLite database",
  "main": "index.js",
  "scripts": {
    "start": "./index.sh",
    "pipeline": "./index.sh",
    "generate": "cd src && node generate-tickers.js",
    "validate": "cd src && node validate-tickers.js",
    "export": "cd src && node export-results.js",
    "export:active": "cd src && node export-results.js --active-only",
    "export:preview": "cd src && node export-results.js --preview",
    "test:validate": "cd src && node validate-tickers.js --limit 100 --dry-run"
  },
  "dependencies": {
    "sqlite3": "^5.1.6",
    "axios": "^1.6.0",
    "yahoo-finance2": "^2.11.3"
  },
  "keywords": ["tickers", "stock", "validation", "bulk"],
  "author": "All-Tickers",
  "license": "MIT"
}
